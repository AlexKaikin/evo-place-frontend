{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "next/core-web-vitals",
    "next",
    "prettier",
    "plugin:storybook/recommended"
  ],
  "rules": {
    "no-console": "warn"
  },
  "ignorePatterns": ["!.storybook"],
  "env": {
    "jest": true,
    "node": true
  }
}
